        },
        "branch" : {
          "type" : "AgencyBranch",
          "description" : "Placement branch"
        }
      },
      "required" : [ "materials", "branch" ]
    },
    "AgencyLocation" : {
      "id" : "AgencyLocation",
      "properties" : {
        "locationId" : {
          "type" : "string",
          "description" : "Location identifier"
        },
        "title" : {
          "type" : "string",
          "description" : "Name of the location"
        }
      },
      "required" : [ "locationId", "title" ]
    },
    "Period" : {
      "id" : "Period",
      "properties" : {
        "from" : {
          "type" : "string",
